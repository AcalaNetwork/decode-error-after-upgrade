{
  "name": "decode-error-after-upgrade",
  "version": "1.0.0",
  "description": "Example of polkadot.js/issue-4557",
  "main": "index.js",
  "scripts": {
    "run:network:relay": "docker-compose -f docker/relay/docker-compose.yml up -d",
    "run:network:dev": "docker-compose -f docker/dev/docker-compose.yml up -d",
    "run:script": "ts-node scripts/repro_defect.ts",
    "run:teardown:relay": "docker-compose -f docker/relay/docker-compose.yml down && docker volume prune -f",
    "run:teardown:dev": "docker-compose -f docker/dev/docker-compose.yml down && docker volume prune -f",
    "run:scenario": "yarn run:network:relay && yarn run:script"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AcalaNetwork/decode-error-after-upgrade.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AcalaNetwork/decode-error-after-upgrade/issues"
  },
  "homepage": "https://github.com/AcalaNetwork/decode-error-after-upgrade#readme",
  "dependencies": {
    "@acala-network/api": "^4.0.1",
    "@polkadot/api": "7.9.1",
    "@polkadot/typegen": "^7.1.1",
    "@polkadot/types": "^7.1.1",
    "prettier": "^2.5.1",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
